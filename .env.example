# ============================================
# AsterDEX Trading Bot Environment Variables
# Copy this file to .env and fill in your values
# ============================================

# ============================================
# AsterDEX API Configuration (REQUIRED)
# Visit: https://www.asterdex.com/api-wallet
# ============================================
ASTERDEX_USER=0xYourMainWalletAddress
ASTERDEX_SIGNER=0xYourAPIWalletAddress
ASTERDEX_PRIVATE_KEY=0xYourAPIWalletPrivateKey
ASTERDEX_API_BASE_URL=https://fapi.asterdex.com

# ============================================
# AI Enhancement System (OPTIONAL)
# Choose one: deepseek or grok
# Set AI_ENABLED=false to disable AI features
# ============================================
AI_ENABLED=true
AI_PROVIDER=deepseek

# DeepSeek AI Configuration (Recommended)
# Visit: https://platform.deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TIMEOUT=30

# Grok AI Configuration (Alternative)
# Visit: https://console.x.ai/
# Uncomment below and comment out DeepSeek if using Grok
# GROK_API_KEY=your_grok_api_key_here
# GROK_API_BASE_URL=https://api.x.ai/v1
# GROK_MODEL=grok-beta
# GROK_TIMEOUT=30

# ============================================
# Trading Configuration
# ============================================
TRADING_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,ASTERUSDT
TRADING_MAX_LEVERAGE=5
TRADING_MAX_POSITION_PERCENT=30
TRADING_MARGIN_TYPE=ISOLATED
TRADING_RECV_WINDOW=50000

# ============================================
# Strategy Configuration
# ============================================
# High-Frequency Strategy (15-minute K-line)
STRATEGY_HIGH_FREQ_ENABLED=true
STRATEGY_HIGH_FREQ_INTERVAL=15m
STRATEGY_HIGH_FREQ_CHECK_INTERVAL=300

# Medium-Frequency Strategy (4-hour K-line)
STRATEGY_MID_FREQ_ENABLED=true
STRATEGY_MID_FREQ_INTERVAL=4h
STRATEGY_MID_FREQ_CHECK_INTERVAL=3600

# ============================================
# Risk Management
# ============================================
RISK_ENABLE_STOP_LOSS=true
RISK_STOP_LOSS_PERCENT=3.0
RISK_ENABLE_TAKE_PROFIT=false
RISK_TAKE_PROFIT_PERCENT=10.0

# ============================================
# Logging Configuration
# ============================================
LOG_LEVEL=INFO
LOG_FILE=/app/logs/trading_bot.log
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=5

# ============================================
# System Configuration
# ============================================
TZ=Asia/Shanghai
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
