# AsterDEX Configuration
ASTERDEX_USER=your_main_wallet_address
ASTERDEX_SIGNER=your_api_wallet_address
ASTERDEX_PRIVATE_KEY=your_api_wallet_private_key
ASTERDEX_API_BASE_URL=https://fapi.asterdex.com

# AI Configuration - DeepSeek (Option 1)
AI_PROVIDER=deepseek
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# AI Configuration - Grok (Option 2)
# AI_PROVIDER=grok
# GROK_API_KEY=your_grok_api_key
# GROK_API_BASE_URL=https://api.x.ai/v1
# GROK_MODEL=grok-beta

# Trading Configuration
TRADING_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,ASTERUSDT
TRADING_MAX_LEVERAGE=5
TRADING_MAX_POSITION_PERCENT=30
TRADING_MARGIN_TYPE=ISOLATED

# Strategy Configuration
STRATEGY_HIGH_FREQ_ENABLED=true
STRATEGY_HIGH_FREQ_INTERVAL=15m
STRATEGY_HIGH_FREQ_CHECK_INTERVAL=300

STRATEGY_MID_FREQ_ENABLED=true
STRATEGY_MID_FREQ_INTERVAL=4h
STRATEGY_MID_FREQ_CHECK_INTERVAL=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=/app/logs/trading_bot.log

# Timezone
TZ=Asia/Shanghai
